    <fields>
        <fullName>Hierarchy_Level__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISBLANK( ReportsToId), 0,
     IF( ISBLANK( ReportsTo.ReportsToId ), 1,
          IF( ISBLANK( ReportsTo.ReportsTo.ReportsToId ), 2,
               IF (ISBLANK ( ReportsTo.ReportsTo.ReportsTo.ReportsToId ), 3,
                    IF (ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 4,
                         IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 5,
                              IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 6,
                                   IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 7,
IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 8,
IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 9,
IF(ISBLANK(ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsTo.ReportsToId), 10, 100)
                                             )
                                        )
                                   )
                              )
                         )
                    )
               )
          )
     )
)</formula>
        <label>Hierarchy Level</label>
